ccnumber = '4904839822485959'
list = [int(x) for x in str(ccnumber)]
print(list)

for x in range(len(list)-2, -1, -2):
    list[x] = list[x] * 2
    if list[x] > 9:
        list[x] = str(list[x])
        list[x] = int(list[x][0]) + int(list[x][1])


print(list)
if sum(list) % 10 == 0:
    print('valid')
